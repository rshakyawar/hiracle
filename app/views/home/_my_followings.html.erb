<br/><br/>
Total No. of Followings : <%= @followings.count %>
<table >
	<tr>
		<th>Number</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
	</tr>
	<% @followings.each_with_index do |follow, index| %>
		<% @following_user = User.find_by_id(follow.follower_id) %>
		<tr>
			<td><%= index+1 %></td>
			<td><%= @following_user.first_name%></td>
			<td><%= @following_user.last_name%></td>
			<td><%= @following_user.email%></td>	
		</tr>
	<%end%>
</table>
<br/>
<%=  link_to 'Back', home_index_path %>